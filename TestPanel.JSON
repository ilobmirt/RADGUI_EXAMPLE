{
   "UISettings1":{
        "TYPE":"CONFIG",
        "CONSOLE_FILTER":{
            "RADGUI_FACTORY":1,
            "RADGUI_ENGINE":1
        },
        "EVENTS":{
            "CUSTOMCLASSES.EXAMPLE_CLASS1.FunctionA":[
                {
                    "EVENT_ID":"btnOPEN"
                },
                {
                    "EVENT_ID":"btnSAVE"
                }
            ],
            "CUSTOMCLASSES.EXAMPLE_CLASS1.FunctionB":[
                {
                    "EVENT_ID":"blnVar1",
                    "VALUE":true
                },
                {
                    "EVENT_ID":"strVar1"
                },
                {
                    "EVENT_ID":"fltVar1"
                }
            ],
            "CUSTOMCLASSES.EXAMPLE_CLASS2.FunctionA":[
                {
                    "EVENT_ID":"btnVIEW"
                }
            ],
            "CUSTOMCLASSES.EXAMPLE_CLASS2.FunctionB":[
                {
                    "EVENT_ID":"blnVar1",
                    "VALUE":false
                },
                {
                    "EVENT_ID":"strVar1"
                },
                {
                    "EVENT_ID":"intVar1"
                }
            ]
        }
   },
    "UIVariables1":{
        "TYPE":"PROPERTIES",
        "DOMAIN":"abcdPropertyDomain",
        "SCOPE":"SCENE",
        "CONTENT":[
            {
                "NAME":"strVar1",
                "TYPE":"STRING",
                "DEFAULT":"TEST STRING"
            },
            {
                "NAME":"intVar1",
                "TEXT":"Health Level",
                "TYPE":"INTEGER",
                "DEFAULT":100,
                "HARD_MIN":0,
                "SOFT_MIN":0,
                "HARD_MAX":110,
                "SOFT_MAX":110,
                "STEP":5
            },
            {
                "NAME":"fltVar1",
                "TEXT":"Pi",
                "TYPE":"FLOAT",
                "DEFAULT":3.14,
		        "HARD_MIN":0.0,
		        "HARD_MAX":20.0
            },
            {
                "NAME":"blnVar1",
                "TEXT":"Did the thing",
                "TYPE":"BOOL",
                "DEFAULT":true
            }
        ]
    },
    "UIButton1":{
        "TYPE":"OPERATOR",
        "TEXT":"DYNAMICALLY GENERATED BUTTON",
        "DOMAIN":"abcdButtonDomain",
        "CLASS":"SimpleButton"
    },
    "UIPanel1":{
        "TYPE":"PANEL",
        "SPACE":"VIEW_3D",
        "REGION":"UI",
        "LABEL":"RAD GUI Panel Example",
        "CONTENT":[
            {
                "TYPE":"LABEL",
                "CONTEXT":"LAYOUT",
                "TEXT":"JSON Panel Example"
            },
            {
                "TYPE":"COLUMN",
                "SAVE":true,
                "ALIGN":true
            },
            {
                "TYPE":"ROW",
                "CONTEXT":"COLUMN",
                "SAVE":true,
                "ALIGN":true
            },
            {
                "TYPE":"OPERATOR",
                "CONTEXT":"ROW",
                "CLASS":"abcdButtonDomain.SimpleButton",
                "TEXT":"Button 1",
                "ICON":"FILEBROWSER",
                "EVENT_ID":"btnOPEN"
            },
            {
                "TYPE":"OPERATOR",
                "CLASS":"abcdButtonDomain.SimpleButton",
                "TEXT":"Button 2",
                "ICON":"FILE_TICK",
                "EVENT_ID":"btnSAVE"
            },
            {
                "TYPE":"ROW",
                "CONTEXT":"COLUMN",
                "SAVE":true,
                "ALIGN":false
            },
            {
                "TYPE":"OPERATOR",
                "CLASS":"abcdButtonDomain.SimpleButton",
                "TEXT":"Button 3",
                "ICON":"VIEW_CAMERA",
                "EVENT_ID":"btnVIEW"
            },
            {
                "TYPE":"ROW",
                "SAVE":true,
                "ALIGN":false
            },
            {
                "TYPE":"ROW",
                "SAVE":true,
                "ALIGN":false
            },
            {
                "TYPE":"PROPERTY",
                "CONTEXT":"ROW",
                "VARIABLE":"scene.abcdPropertyDomain.strVar1"
            },
            {
                "TYPE":"ROW",
                "CONTEXT":"COLUMN",
                "SAVE":true,
                "ALIGN":false
            },
            {
                "TYPE":"PROPERTY",
                "CONTEXT":"ROW",
                "VARIABLE":"scene.abcdPropertyDomain.intVar1"
            },
            {
                "TYPE":"ROW",
                "CONTEXT":"COLUMN",
                "SAVE":true,
                "ALIGN":false
            },
            {
                "TYPE":"PROPERTY",
                "CONTEXT":"ROW",
                "VARIABLE":"scene.abcdPropertyDomain.fltVar1"
            },
            {
                "TYPE":"ROW",
                "CONTEXT":"COLUMN",
                "SAVE":true,
                "ALIGN":false
            },
            {
                "TYPE":"PROPERTY",
                "CONTEXT":"ROW",
                "VARIABLE":"scene.abcdPropertyDomain.blnVar1"
            },
            {
                "TYPE":"ROW",
                "CONTEXT":"COLUMN",
                "SAVE":true,
                "ALIGN":false
            },
            {
                "TYPE":"LABEL",
                "TEXT":"End of Panel"
            }
        ]
    }
}